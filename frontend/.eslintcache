t._id || product._id !== match.params.id) {\n      dispatch(listProductDetails(match.params.id));\n      dispatch({ type: PRODUCT_CREATE_REVIEW_RESET });\n    }\n  }, [dispatch, match, successProductReview]);\n\n  const addToCartHandler = () => {\n    history.push(`/cart/${match.params.id}?qty=${qty}`);\n  };\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n    dispatch(\n      createProductReview(match.params.id, {\n        rating,\n        comment,\n      })\n    );\n  };\n\n  return (\n    <>\n      <Link className='btn btn-light my-3' to='/'>\n        Retour\n      </Link>\n      {loading ? (\n        <Loader />\n      ) : error ? (\n        <Message variant='danger'>{error}</Message>\n      ) : (\n        <>\n          <Meta title={product.name} />\n          <Row>\n            <Col md={6}>\n              <Image src={product.image} alt={product.name} fluid />\n            </Col>\n            <Col md={3}>\n              <ListGroup variant='flush'>\n                <ListGroup.Item>\n                  <h3>{product.name}</h3>\n                </ListGroup.Item>\n                <ListGroup.Item>\n                  <Rating\n                    value={product.rating}\n                    text={`${product.numReviews} avis`}\n                  />\n                </ListGroup.Item>\n                <ListGroup.Item>Prix: {product.price}€</ListGroup.Item>\n                <ListGroup.Item>\n                  Description: {product.description}\n                </ListGroup.Item>\n              </ListGroup>\n            </Col>\n            <Col md={3}>\n              <Card>\n                <ListGroup variant='flush'>\n                  <ListGroup.Item>\n                    <Row>\n                      <Col>Prix:</Col>\n                      <Col>\n                        <strong>${product.price}</strong>\n                      </Col>\n                    </Row>\n                  </ListGroup.Item>\n\n                  <ListGroup.Item>\n                    <Row>\n                      <Col>Status:</Col>\n                      <Col>\n                        {product.countInStock > 0\n                          ? \"En Stock\"\n                          : \"Rupture de stock\"}\n                      </Col>\n                    </Row>\n                  </ListGroup.Item>\n\n                  {product.countInStock > 0 && (\n                    <ListGroup.Item>\n                      <Row>\n                        <Col>Quantité</Col>\n                        <Col>\n                          <Form.Control\n                            as='select'\n                            value={qty}\n                            onChange={(e) => setQty(e.target.value)}\n                          >\n                            {[...Array(product.countInStock).keys()].map(\n                              (x) => (\n                                <option key={x + 1} value={x + 1}>\n                                  {x + 1}\n                                </option>\n                              )\n                            )}\n                          </Form.Control>\n                        </Col>\n                      </Row>\n                    </ListGroup.Item>\n                  )}\n\n                  <ListGroup.Item>\n                    <Button\n                      onClick={addToCartHandler}\n                      className='btn-block'\n                      type='button'\n                      disabled={product.countInStock === 0}\n                    >\n                      Ajouter au panier\n                    </Button>\n                  </ListGroup.Item>\n                </ListGroup>\n              </Card>\n            </Col>\n          </Row>\n          <Row>\n            <Col md={6}>\n              <h2 className='ml-3 mt-5'>Avis</h2>\n              {product.reviews.length === 0 && <Message>Aucun avis</Message>}\n              <ListGroup variant='flush'>\n                {product.reviews.map((review) => (\n                  <ListGroup.Item key={review._id}>\n                    <strong>{review.name}</strong>\n                    <Rating value={review.rating} />\n                    <p>{review.createdAt.substring(0, 10)}</p>\n                    <p>{review.comment}</p>\n                  </ListGroup.Item>\n                ))}\n                <ListGroup.Item>\n                  <h2>Questions et réponses de clients</h2>\n                  {successProductReview && (\n                    <Message variant='success'>Avis publié avec succès</Message>\n                  )}\n                  {loadingProductReview && <Loader />}\n                  {errorProductReview && (\n                    <Message variant='danger'>{errorProductReview}</Message>\n                  )}\n                  {userInfo ? (\n                    <Form onSubmit={submitHandler}>\n                      <Form.Group controlId='rating'>\n                        <Form.Label>Note</Form.Label>\n                        <Form.Control\n                          as='select'\n                          value={rating}\n                          onChange={(e) => setRating(e.target.value)}\n                        >\n                          <option value=''>Choix...</option>\n                          <option value='1'>1 - Etoile</option>\n                          <option value='2'>2 - Etoiles</option>\n                          <option value='3'>3 - Etoiles</option>\n                          <option value='4'>4 - Etoiles</option>\n                          <option value='5'>5 - Etoiles</option>\n                        </Form.Control>\n                      </Form.Group>\n                      <Form.Group controlId='comment'>\n                        <Form.Label>Commentaire</Form.Label>\n                        <Form.Control\n                          as='textarea'\n                          row='3'\n                          value={comment}\n                          onChange={(e) => setComment(e.target.value)}\n                        ></Form.Control>\n                      </Form.Group>\n                      <Button\n                        disabled={loadingProductReview}\n                        type='submit'\n                        variant='primary'\n                      >\n                        Envoyer\n                      </Button>\n                    </Form>\n                  ) : (\n                    <Message>\n                      Merci de vous <Link to='/login'>connecter</Link> pour\n                      ecrire un avis{\" \"}\n                    </Message>\n                  )}\n                </ListGroup.Item>\n              </ListGroup>\n            </Col>\n          </Row>\n        </>\n      )}\n    </>\n  );\n};\n\nexport default ProductScreen;\n",["186","187"],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/store.js",[],["188","189"],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/reducers/productReducers.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/actions/productActions.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/constants/productConstants.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/reducers/cartReducers.js",[],["190","191"],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/CartScreen.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/actions/cartActions.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/reducers/userReducers.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/LoginScreen.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/actions/userActions.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/constants/userConstants.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/RegisterScreen.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/ProfileScreen.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/ShippingScreen.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/