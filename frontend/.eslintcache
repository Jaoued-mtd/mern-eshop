ddDecimals(\n      order.orderItems.reduce((acc, item) => acc + item.price * item.qty, 0)\n    );\n  }\n\n  useEffect(() => {\n    if (!userInfo) {\n      history.push(\"/login\");\n    }\n\n    const addPayPalScript = async () => {\n      const { data: clientId } = await axios.get(\"/api/config/paypal\");\n      const script = document.createElement(\"script\");\n      script.type = \"text/javascript\";\n      script.src = `https://www.paypal.com/sdk/js?client-id=${clientId}`;\n      script.async = true;\n      script.onload = () => {\n        setSdkReady(true);\n      };\n      document.body.appendChild(script);\n    };\n\n    if (!order || successPay || successDeliver || order._id !== orderId) {\n      dispatch({ type: ORDER_PAY_RESET });\n      dispatch({ type: ORDER_DELIVER_RESET });\n      dispatch(getOrderDetails(orderId));\n    } else if (!order.isPaid) {\n      if (!window.paypal) {\n        addPayPalScript();\n      } else {\n        setSdkReady(true);\n      }\n    }\n  }, [dispatch, orderId, successPay, successDeliver, order]);\n\n  const successPaymentHandler = (paymentResult) => {\n    console.log(paymentResult);\n    dispatch(payOrder(orderId, paymentResult));\n  };\n\n  const deliverHandler = () => {\n    dispatch(deliverOrder(order));\n  };\n\n  return loading ? (\n    <Loader />\n  ) : error ? (\n    <Message variant='danger'>{error}</Message>\n  ) : (\n    <>\n      <h1>Commande {order._id}</h1>\n      <Row>\n        <Col md={8}>\n          <ListGroup variant='flush'>\n            <ListGroup.Item>\n              <h2>Livraison</h2>\n              <p>\n                <strong>Nom: </strong> {order.user.name}\n              </p>\n              <p>\n                <strong>Email: </strong>{\" \"}\n                <a href={`mailto:${order.user.email}`}>{order.user.email}</a>\n              </p>\n              <p>\n                <strong>Adresse:</strong>\n                {order.shippingAddress.address}, {order.shippingAddress.city}{\" \"}\n                {order.shippingAddress.postalCode},{\" \"}\n                {order.shippingAddress.country}\n              </p>\n              {order.isDelivered ? (\n                <Message variant='success'>\n                  Livraison le {order.deliveredAt}\n                </Message>\n              ) : (\n                <Message variant='danger'>Aucune livraison en cours</Message>\n              )}\n            </ListGroup.Item>\n\n            <ListGroup.Item>\n              <h2>Paiement</h2>\n              <p>\n                <strong>Methode: </strong>\n                {order.paymentMethod}\n              </p>\n              {order.isPaid ? (\n                <Message variant='success'>Payé le {order.paidAt}</Message>\n              ) : (\n                <Message variant='danger'>Aucun paiement en cours </Message>\n              )}\n            </ListGroup.Item>\n\n            <ListGroup.Item>\n              <h2>Commande</h2>\n              {order.orderItems.length === 0 ? (\n                <Message>Votre commande ne comporte aucun article.</Message>\n              ) : (\n                <ListGroup variant='flush'>\n                  {order.orderItems.map((item, index) => (\n                    <ListGroup.Item key={index}>\n                      <Row>\n                        <Col md={1}>\n                          <Image\n                            src={item.image}\n                            alt={item.name}\n                            fluid\n                            rounded\n                          />\n                        </Col>\n                        <Col>\n                          <Link to={`/product/${item.product}`}>\n                            {item.name}\n                          </Link>\n                        </Col>\n                        <Col md={4}>\n                          {item.qty} x {item.price}€ = {item.qty * item.price}€\n                        </Col>\n                      </Row>\n                    </ListGroup.Item>\n                  ))}\n                </ListGroup>\n              )}\n            </ListGroup.Item>\n          </ListGroup>\n        </Col>\n        <Col md={4}>\n          <Card>\n            <ListGroup variant='flush'>\n              <ListGroup.Item>\n                <h2>PRIX</h2>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Articles</Col>\n                  <Col>{order.itemsPrice}€</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Frais de livraison</Col>\n                  <Col>{order.shippingPrice}€</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Taxe</Col>\n                  <Col>{order.taxPrice}€</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Total</Col>\n                  <Col>{order.totalPrice}€</Col>\n                </Row>\n              </ListGroup.Item>\n              {!order.isPaid && (\n                <ListGroup.Item>\n                  {loadingPay && <Loader />}\n                  {!sdkReady ? (\n                    <Loader />\n                  ) : (\n                    <PayPalButton\n                      amount={order.totalPrice}\n                      onSuccess={successPaymentHandler}\n                    />\n                  )}\n                </ListGroup.Item>\n              )}\n              {loadingDeliver && <Loader />}\n              {userInfo &&\n                userInfo.isAdmin &&\n                order.isPaid &&\n                !order.isDelivered && (\n                  <ListGroup.Item>\n                    <Button\n                      type='button'\n                      className='btn btn-block'\n                      onClick={deliverHandler}\n                    >\n                      Confirmer la livraison\n                    </Button>\n                  </ListGroup.Item>\n                )}\n            </ListGroup>\n          </Card>\n        </Col>\n      </Row>\n    </>\n  );\n};\n\nexport default OrderScreen;\n","/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/UserListScreen.js",[],["193","194"],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/UserEditScreen.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/ProductListScreen.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/ProductEditScreen.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/screens/OrderListScreen.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/components/SearchBox.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/components/ProductCarousel.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/components/Paginate.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/components/Meta.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/components/CheckoutSteps.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/components/Loader.js",[],"/Users/jaouedlb/Documents/Mission/eshop/frontend/src/components/Message.js",[],{"ruleId":"195","replacedBy":"196"},{"ruleId":"197","replacedBy":"198"},{"ruleId":"195","replacedBy":"199"},{"ruleId":"197","replacedBy":"200"},{"ruleId":"195","replacedBy":"201"},{"ruleId":"197","replacedBy":"202"},{"ruleId":"203","severity":1,"message":"204","line":52,"column":6,"nodeType":"205","endLine":52,"endColumn":45,"suggestions":"206"},{"ruleId":"195","replacedBy":"207"},{"ruleId":"197","replacedBy":"208"},{"ruleId":"195","replacedBy":"209"},{"ruleId":"197","replacedBy":"210"},{"ruleId":"195","replacedBy":"211"},{"ruleId":"197","replacedBy":"212"},{"ruleId":"203","severity":1,"message":"213","line":77,"column":6,"nodeType":"205","endLine":77,"endColumn":60,"suggestions":"214"},{"ruleId":"195","replacedBy":"215"},{"ruleId":"197","r